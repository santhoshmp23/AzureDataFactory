							"name": "DestDistinctCity"
						},
						{
							"linkedService": {
								"referenceName": "SqlServerAzure",
								"type": "LinkedServiceReference"
							},
							"name": "DestDistinctVenue"
						}
					],
					"transformations": [
						{
							"name": "SelectOnlyTeam1"
						},
						{
							"name": "SelectOnlyTeam2"
						},
						{
							"name": "UnionTwoTeams"
						},
						{
							"name": "DistinctTeams"
						},
						{
							"name": "SelectOnlyCity"
