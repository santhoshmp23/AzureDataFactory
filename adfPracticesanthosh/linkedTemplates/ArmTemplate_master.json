{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adfPracticesanthosh"
		},
		"SQLConTSC_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SQLConTSC'"
		},
		"SqlDbCon_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SqlDbCon'"
		},
		"SqlServerAzure_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SqlServerAzure'"
		},
		"ls_Pipeline_logs_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_Pipeline_logs'"
		},
		"ls_RawFile_DL_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_RawFile_DL'"
		},
		"AzureSQLServer_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSQLServer'",
			"defaultValue": "Integrated Security=False;Data Source=@{linkedService().pLsServerName};Initial Catalog=@{linkedService().pLsDatabaseName};User ID=@{linkedService().pLsUserName};Password=@{linkedService().pLSPassword}"
		},
		"ls_RawFile_DL_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://saadfpractivedl.dfs.core.windows.net/"
		},
		"default_properties_gP_CreatedBy_value": {
			"type": "string",
			"defaultValue": "SanthoshAdfPractice"
		},
		"default_properties_pDatalakeName_value": {
			"type": "string",
			"defaultValue": "https://saadfpractivedl.dfs.core.windows.net/"
		},
		"default_properties_pRawFileName_value": {
			"type": "string",
			"defaultValue": "zomato.csv"
		},
		"default_properties_GTurbineOEM_value": {
			"type": "string",
			"defaultValue": "Gamesa"
		},
		"default_properties_gPDatabaseServerName_value": {
			"type": "string",
			"defaultValue": "."
		},
		"default_properties_gPDatabaseName_value": {
			"type": "string",
			"defaultValue": "."
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adfPracticesanthosh_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"SQLConTSC_connectionString": {
						"value": "[parameters('SQLConTSC_connectionString')]"
					},
					"SqlDbCon_connectionString": {
						"value": "[parameters('SqlDbCon_connectionString')]"
					},
					"SqlServerAzure_connectionString": {
						"value": "[parameters('SqlServerAzure_connectionString')]"
					},
					"ls_Pipeline_logs_connectionString": {
						"value": "[parameters('ls_Pipeline_logs_connectionString')]"
					},
					"ls_RawFile_DL_accountKey": {
						"value": "[parameters('ls_RawFile_DL_accountKey')]"
					},
					"AzureSQLServer_connectionString": {
						"value": "[parameters('AzureSQLServer_connectionString')]"
					},
					"default_properties_gP_CreatedBy_value": {
						"value": "[parameters('default_properties_gP_CreatedBy_value')]"
					},
					"default_properties_pDatalakeName_value": {
						"value": "[parameters('default_properties_pDatalakeName_value')]"
					},
					"default_properties_pRawFileName_value": {
						"value": "[parameters('default_properties_pRawFileName_value')]"
					},
					"default_properties_GTurbineOEM_value": {
						"value": "[parameters('default_properties_GTurbineOEM_value')]"
					},
					"default_properties_gPDatabaseServerName_value": {
						"value": "[parameters('default_properties_gPDatabaseServerName_value')]"
					},
					"default_properties_gPDatabaseName_value": {
						"value": "[parameters('default_properties_gPDatabaseName_value')]"
					},
					"ls_RawFile_DL_properties_typeProperties_url": {
						"value": "[parameters('ls_RawFile_DL_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "adfPracticesanthosh_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adfPracticesanthosh_ArmTemplate_0')]"
			]
		}
	]
}